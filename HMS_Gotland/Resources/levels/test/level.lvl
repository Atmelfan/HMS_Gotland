//Level name
&name test_level

//Level data(stored as .obj)
&obj ship.obj

//&exit, exit to another level
&exit start true 0 0 0

//sets player pos and angle
&player pos 0 15 0
//&player angle 0 0 0

//Add a test entity
&entity default 0 15 9 {
	pos 0 20 0
}

&entity default -2.5 20 0

//Compound entity
&entity WAR {
	&model war.md2
	&ai attack
	&health 20
}

#event player.death {
	&gui display glass_shattered
	&gui prompt "OFFLINE"
	&delay 1000
	&gui clear
	&player pos 0 10 -15
}
